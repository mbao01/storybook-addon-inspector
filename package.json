{
  "name": "storybook-addon-inspector",
  "version": "0.0.5",
  "description": "Inspects the computed css properties applied to an element in storybook",
  "keywords": [
    "inspector",
    "css",
    "variables",
    "storybook-addons"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbao01/storybook-addon-inspector.git"
  },
  "type": "module",
  "license": "MIT",
  "author": "Ayomide Bakare <bakkareayomideo@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js",
      "require": "./dist/preview.cjs"
    },
    "./preset": "./dist/preset.cjs",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "prerelease": "zx scripts/prepublish-checks.js",
    "release": "auto shipit",
    "start": "run-p build:watch \"storybook dev -p 6006 --quiet\"",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage.enabled=true",
    "test:coverage:watch": "vitest --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.4",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.0",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-links": "8.6.12",
    "@storybook/blocks": "8.6.12",
    "@storybook/components": "^8.6.12",
    "@storybook/global": "5.0.0",
    "@storybook/icons": "^1.4.0",
    "@storybook/manager-api": "^8.6.12",
    "@storybook/preview-api": "^8.6.12",
    "@storybook/react": "8.6.12",
    "@storybook/react-vite": "8.6.12",
    "@storybook/test": "8.6.12",
    "@storybook/types": "^8.6.12",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^18.19.86",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-react": "^4.4.0",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "auto": "^11.3.0",
    "boxen": "^7.1.1",
    "chalk": "^5.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dedent": "^1.5.3",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.488.0",
    "npm-run-all2": "^6.2.6",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "8.6.12",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tiny-invariant": "^1.3.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^5.4.18",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vitest": "^3.1.1",
    "zx": "^7.2.3"
  },
  "peerDependencies": {
    "storybook": "^8.6.12"
  },
  "overrides": {
    "storybook": "8.6.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "storybook": {
    "displayName": "Inspector",
    "supportedFrameworks": [
      "react",
      "vue",
      "angular",
      "web-components",
      "ember",
      "html",
      "svelte",
      "preact",
      "react-native"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
